<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs></exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.appArgs></exec.appArgs>
                <exec.mainClass>${packageClassName}</exec.mainClass>
                <exec.executable>java</exec.executable>
                <Env.FOO>bar</Env.FOO>
                <mySqlPassword>root</mySqlPassword>
                <mySqlUrl>jdbc:mysql://localhost:3306/wallet</mySqlUrl>
                <security.jwt.secret>secretito</security.jwt.secret>
                <security.jwt.issuer>issuercito</security.jwt.issuer>
                <security.jwt.ttlMillis>3600000</security.jwt.ttlMillis>
            </properties>
        </action>
    </actions>
